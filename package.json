{
  "name": "acc-creator-react",
  "version": "1.3.5",
  "description": "League of Legends accounts creator",
  "author": "Dmitriy Frolov",
  "engines": {
    "node": ">=14"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lociero/League-of-Legends-Accounts-Creator.git"
  },
  "dependencies": {
    "@babel/plugin-syntax-import-meta": "^7.8.3",
    "@fortawesome/fontawesome-free": "^5.12.1",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "axios": "^0.19.2",
    "bluebird": "^3.7.2",
    "body-parser": "^1.19.0",
    "bootstrap": "^4.4.1",
    "bootswatch": "^4.4.1",
    "custom-electron-titlebar": "^3.2.2-hotfix62",
    "express": "^4.17.1",
    "generate-password": "^1.5.1",
    "jquery": "^3.5.0",
    "open": "^7.0.2",
    "popper.js": "^1.16.1",
    "react": "^16.13.0",
    "react-copy-to-clipboard": "^5.0.2",
    "react-dom": "^16.13.0",
    "react-redux": "^7.2.0",
    "redux": "^4.0.5",
    "redux-actions": "^2.6.5",
    "socks-proxy-agent": "^5.0.0"
  },
  "main": "public/electron-starter.js",
  "homepage": "./",
  "build": {
    "appId": "acc.creator",
    "win": {
      "target": "portable",
      "icon": "public/icon.ico",
      "files":["!**/*.zip", "build", "public", "server"]
    }
  },
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test --env=jsdom",
    "electron": "electron .",
    "buildElectron": "electron-packager ./ creator --platform=win32 --arch=x64 --asar",
    "pack": "build --dir",
    "dist": "electron-builder"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-optional-chaining": "^7.9.0",
    "customize-cra": "^0.9.1",
    "electron": "^8.0.3",
    "electron-builder": "^22.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-react-app": "^5.2.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.20.0",
    "prettier": "^2.0.5",
    "react-app-rewired": "^2.1.6"
  }
}
